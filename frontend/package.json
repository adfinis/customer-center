{
  "name": "customer-center",
  "description": "Adfinis customer support portal with Timed integration.",
  "repository": "https://github.com/adfinis/customer-center",
  "version": "3.2.6",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Adfinis AG",
    "email": "info@adfinis.com",
    "url": "https://adfinis.com/"
